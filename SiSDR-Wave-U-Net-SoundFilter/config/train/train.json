{
    "seed": 0,
    "description": "...des",

    "root_dir": "~/Ameya/Wave-U-Net/SiSDR-Wave-U-Net-SoundFilter/Experiments/Fuss-Wave-U-Net",
    "cudnn_deterministic": false,
    "trainer": {
        "module": "trainer.trainer",
        "main": "Trainer",
        "epochs": 100,
        "save_checkpoint_interval": 5,
        "validation": {
            "interval": 10,
            "find_max": true,
            "custom": {
                "visualize_audio_limit": 20,
                "visualize_waveform_limit": 20,
                "visualize_spectrogram_limit": 20,
                "sample_length": 8000
            }
        }
    },
    "model": {
        "module": "model.unet_condition_twoshot",
        "main": "Model",
        "args": {}
    },
    "loss_function": {
        "module": "model.loss",
        "main": "sisdr_loss",
        "args": {}
    },
    "optimizer": {
        "lr": 0.001,
        "beta1": 0.9,
        "beta2": 0.999
    },
    "train_dataset": {
        "module": "dataset.waveform_dataset",
        "main": "Dataset",
        "args": {
            "dataset": "~/Ameya/Wave-U-Net/Fuss_Data/train_dataset_temp.txt",
            "limit": null,
            "offset": 0,
            "sample_length": 8000,
            "mode": "train"
        }
    },
    "validation_dataset": {
        "module": "dataset.waveform_dataset",
        "main": "Dataset",
        "args": {
            "dataset": "~/Ameya/Wave-U-Net/Fuss_Data/val_dataset_temp.txt",
            "limit": 400,
            "offset": 0,
	    "sample_length": 8000,
            "mode": "validation"
        }
    },
    "train_dataloader": {
        "batch_size": 32,
        "num_workers": 12,
        "shuffle": true,
        "pin_memory": true
    }
}

